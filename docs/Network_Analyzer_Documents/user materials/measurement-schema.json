{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
      "timestamp": {
        "type": "string",
        "format": "date-time",
        "description": "ISO 8601 formatted timestamp for when the measurement was recorded."
      },
      "frequency": {
        "type": "number",
        "minimum": 0,
        "description": "Frequency of the measured signal in Hz."
      },
      "amplitude": {
        "type": "number",
        "minimum": 0,
        "description": "Peak-to-peak amplitude in volts."
      },
      "phase": {
        "type": "number",
        "description": "Phase shift in degrees."
      }
    },
    "required": ["timestamp", "frequency", "amplitude", "phase"]
  }
  